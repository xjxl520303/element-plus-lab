{
  "extends": "@element-plus-lab/tsconfig/app.json",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./examples/*"]
    },
    "types": ["vite/client", "vue-vine/macros", "unplugin-icons/types/vue", "unplugin-turbo-console/client"]
  },
  "include": [
    "**/*.d.ts",
    "examples/**/*.ts",
    "examples/**/*.tsx",
    "examples/**/*.vue",
    "examples/**/*.vine.ts",
    ".vitepress/**/*.ts",
    ".vitepress/**/*.mts",
    ".vitepress/**/*.vue",
    ".vitepress/**/*.d.ts"
  ]
}
